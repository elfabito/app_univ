<div class="row sign-up-form">
  <div class="col s6 m6">
    <div class="card grey darken-1">
      <div class="card-content white-text">
        <span class="card-title"> Name : <%= @student.name %></span>
        <p> Email : <%= @student.email %></p>
        <p>Course Enrollments: None </p>
      </div>
    </div>
    <%= link_to '<i class="material-icons right">create</i>"Edit your Profile"'.html_safe, edit_student_path(@student), class: "waves-effect blue accent-4 btn" %>
  </div>
</div>
